                         325 --168:         uartparams.RxEnable    = R_RLIN3_RX_ENABLED;
                         326 --169:         uartparams.Detection   = R_RLIN3_DET_BIT_OVR;
                         327 --170:         R_RLIN3_Init(Unit, &uartparams);
                         328 --171:     }
