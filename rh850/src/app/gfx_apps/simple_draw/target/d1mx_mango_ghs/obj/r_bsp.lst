                         425 --276: */
                         426 --278: void R_BSP_PrintClkCfg(void)
                         427 --279: {
                         428 --280:     uint32_t             i, frq; 
                         429 --281:     r_dev_ClkSelConfig_t clk_cfg; 
                         430 --282:     
                         431 --283:     R_BSP_STDIO_Printf("Board: %s%s, Board Version: %s\n",  R_DEV_GetDevName(), 
                         432 --284:                                                             R_BSP_GetBoardName(), 
                         433 --285:                                                             R_BSP_GetBoardVersion());
                         434 --287:     /* The following check cannot be done at compile time as 'sizeof()' is unknown to 
                         435 --288:        the preprocessor. 
                         436 --289:        On the other hand, MISRA will compain an invariant boolen expression. (Rule 13.7)*/
                         437 --290:   #pragma ghs startnomisra
                         438 --291:     if ( (sizeof(locSrcNames) / sizeof(char*)) != R_DEV_BUS_ALL) {
                         439 --292:         R_BSP_STDIO_Printf("\nDOCU MISMATCH!?! Please adapt Array <locSrcNames> in r_bsp.c to Enum <r_dev_Bus_t>\n");
                         440 --293:     }
                         441 --294:     if ( (sizeof(locSelNames) / sizeof(char*)) != R_DEV_CKS_ALL) {
                         442 --295:         R_BSP_STDIO_Printf("\nDOCU MISMATCH!?! Please adapt Array <locSelNames> in r_bsp.c to Enum <r_dev_ClkSel_t>\n");
                         443 --296:     }
                         444 --297:     if ( (sizeof(locSrcSelNames) / sizeof(char*)) != R_DEV_CKS_SRC_ALL) {
                         445 --298:         R_BSP_STDIO_Printf("\nDOCU MISMATCH!?! Please adapt Array <locSrcSelNames> in r_bsp.c to Enum <r_dev_CksSrc_t>\n");
                         446 --299:     }
                         447 --300:   #pragma ghs endnomisra
                         448 --302:     R_BSP_STDIO_Printf("\n");
                         449 --303:     R_BSP_STDIO_Printf("******* Configured Clock sources *************\n");
                         450 --304:     for (i = (uint32_t)R_DEV_CLK_PLL0; i < (uint32_t)R_DEV_BUS_ALL; i++)
                         451 --305:     {
                         452 --306:         frq = R_DEV_BusFrequencyHz((r_dev_Bus_t)i); 
                         453 --307:         if (frq > 1000000)
                         454 --308:         {
                         455 --309:             R_BSP_STDIO_Printf("%-13s : %3d MHz\n",  locSrcNames[i],  frq/1000000u);
                         456 --310:         }
                         457 --311:         else if (frq > 0)
                         458 --312:         {
                         459 --313:             R_BSP_STDIO_Printf("%-13s : %3d KHz\n",  locSrcNames[i],  frq/1000u);
                         460 --314:         }
                         461 --315:         else 
                         462 --316:         {
                         463 --317:              R_BSP_STDIO_Printf("%-13s :     OFF\n",  locSrcNames[i]);
                         464 --318:            
                         465 --319:         }
                         466 --320:     }
                         467 --321:     R_BSP_STDIO_Printf("\n");
                         468 --322:     
                         469 --323:     R_BSP_STDIO_Printf("******* Configured Clocks     *************\n");
                         470 --324:     for (i = (uint32_t)R_DEV_CKS_WDT0; i < (uint32_t)R_DEV_CKS_ALL; i++)
                         471 --325:     {
                         472 --326:         if ((uint32_t)R_DEV_CKS_LAST != i) 
